<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.patientportaltoolkit">
    <class table="CANCER_COMMUNITY_RESOURCES" name="CancerCommunityResources">
        <id name="id" type="int" column="id"  unsaved-value="0">
            <generator class="native" />
        </id>
        <property name = "cancerType" column = "cancerType" type = "java.lang.String"/>
        <property name = "usefulContacts" column = "usefulcontacts" type = "java.lang.String"/>
        <property name = "resources" column = "resources" type = "java.lang.String"/>
    </class>
</hibernate-mapping>